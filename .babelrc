{
  "env": {
    "development": {
      "presets" : [["@babel/preset-env",   {
          "useBuiltIns": "usage",
          "corejs": "3.0.0",
          "shippedProposals": true,
          "targets": {
            "node": "10.15"
        }
      }], "@babel/preset-react"],
      "plugins" : ["react-hot-loader/babel","@babel/plugin-transform-runtime"]
    },
    "production": {
      "presets" : [["@babel/preset-env", { "modules": false, "loose": true }], "@babel/preset-react"]
    }
  }
}